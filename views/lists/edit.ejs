<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/lists/edit.css" />
<%- include('../partials/nav') %>

<main>
  <h1><%= title %></h1>
  <form 
  id="new-form" 
  action="/lists/<%= list._id %>?_method=PUT" 
  method="POST"
  >
  <label for="title-input">List Name:</label>
    <input type="text" value="<%= list.title %>" name="title" id="title-input">
    <button id="submit">Rename List</button>
  </form>
</main>


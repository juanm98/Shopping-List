<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/lists/index.css" />
<%- include('../partials/nav') %>

<main>
  <h1>Shopping List</h1>
  <table>
    <thead>
      <tr>
        <th>List Name</th>
      </tr>
    </thead>
    <tbody>
      <% lists.forEach(list => { %>
        <tr>
          <td><%= list.title %></td>
        </tr>
        <% }) %>
    </tbody>
  </table>
</main>

<%- include('../partials/footer') %>